.problem
    &__catalog 
        margin-top 127px
        margin-bottom -5px
        &-list 
            display flex
            align-items flex-start
            flex-wrap: wrap
            padding 0
            margin 0
            list-style-type none
            margin-right -66px
            margin-left -66px
            margin-top -60px
        @media screen and (max-width: 1366px)
            &-list 
                margin-left -30px
                margin-right -30px
        @media screen and (max-width: 960px)
            &-list
                justify-content space-between
                margin-left 0px
                margin-right 0px
        @media phone-portrait
            &-list 
                margin-top 0px
        &-item 
            width calc(100%/2 - 132px)
            margin: 0px 66px 
            &:not(:nth-last-child(-n+2))
                margin-bottom 110px
        @media screen and (max-width: 1366px)
            &-item
                width calc(100%/3 - 60px)
                margin-left 30px
                margin-right 30px
        @media screen and (max-width: 960px)
            &-item
                width: calc(50% - 15px)
                margin-left 0
                margin-right 0
                &:not(:nth-last-child(-n+2))
                    margin-bottom 60px
                    margin-top 0
        @media phone-portrait
            &-item 
                width 100%
                margin: 30px 0
        @media phone
            &-item 
                &:not(:nth-last-child(-n+2))
                    margin-bottom 50px
        &-img 
            display flex
            width 100%
            background-position center
            background-size: cover
            background-repeat no-repeat
            &:before
                content: ''
                display block
                float: left
                width 100%
                padding-bottom 100%
            img 
                display none
        &-name 
            font-family defaultFont
            text-transform uppercase
            font-size 13px
            text-align: center
            display block 
            color: black
            margin-top 20px
        &-link 
            text-decoration: none
    @media phone-portrait
        &__catalog
            margin-top 40px

            
